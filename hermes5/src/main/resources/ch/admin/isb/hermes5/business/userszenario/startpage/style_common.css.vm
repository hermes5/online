body {
	font-family: Arial, Helvetica, Verdana, Lucida, sans-serif;
	color: black;
	font-size: 0.75em;
}

th{
	font-size: 0.92em;
	font-weight: bold;
}

td{
	font-size: 0.92em;
}

a:link, a:visited {
	text-decoration: underline;
	color: #006699;
}


h1, .h1 {
	font-size: 1.5em;
	font-weight: bold;
	color: #008ACD;
	padding-right: 3em;
	margin-bottom: 1em;
}

h2, .h2 {
	font-size: 1.2em;
	font-weight: bold;
	color: #008ACD;
	margin-top: 1em;
	margin-bottom: .8em;
	display: block;
}

h3, .h3 {
	font-size: 1.2em;
	font-weight: normal;
	color: #008ACD;
	display: block;
}

.relationshiptable a:link, .relationshiptable a:visited {
	text-decoration: none;
	color: #000000;
}

.relationshiptable a:hover {
	text-decoration: underline;
}


.title{
	margin-bottom: 1em;
}
 

.overviewtable {
	width: 100%;
	border-spacing: 0px 3px;
	table-layout: fixed;
}

.overviewtable a:link, .overviewtable a:visited {
	color: #000000;
	text-decoration: none;
}

.overviewtable a:hover {
	text-decoration: underline;
}

.listtable {
	width: 100%;
	border-spacing: 0px 0px;
}

.listtable td{
	font-size: 1em;
	border-right: 1px solid #B2C400;
	color: #555;
	padding: 2px 5px 3px;
}

.listtable th{
	background-color: #B2C400;
	font-weight: bold;
	padding: 7px 7px 8px;
	text-align: left;
	border-right: 1px solid #FFFFFF;
	line-height: 1.3em;
}

.listtable a{
	margin-right: .5em;
	text-decoration: none;
	color: #000;	
}
.listtable a:hover {
	text-decoration: underline;
}

.listtable .checkbox {
	text-align: center;
}

.relationshiptable {
	padding-top: 1em;
	width: 100%;
	border-spacing: 0px 0px;
}
.relationshiptable td {
	padding: 5px;
	border-right: 1px solid #B2C400;
	vertical-align: top;
}
.relationshiptable th {
	padding: 5px;
	background-color: #B2C400;
	border-right: 1px solid #FFFFFF;
	vertical-align: top;
	text-align: left;
}
.relationshiptable .mitarbeitend {
	display: block;
}

.aufgabe-purpose {
	font-style: italic;
}

.overviewtable .cell-elements {
	vertical-align:top;
}

.cell-elements-box{
	display:block;
	padding:5px;
}

.cell-elements {
	border-top-color: white;
	border-right: 1px solid #B2C400;
	color: #555;
	padding: 7px 5px 8px;
}

.cell-phase {
	background-color: #B2C400;
	font-weight: bold;
	padding: 7px 7px 8px;
	text-align: left;
	border-right: 1px solid #FFFFFF;
	line-height: 1.3em;
}

.cell-modul {
	font-weight: bold;
	padding: 7px 7px 8px;
	text-align: left;
	vertical-align: top;
	border-right: 1px solid #FFFFFF;
	line-height: 1.3em;
	background-color: #B2C400;
}

.last, .relationshiptable .last, .listtable .last{
	border-right: 1px solid #FFFFFF;
}



.cell-modul .cell-elements-box {
	background-color: #B2C400; 
}

.cell-modul .cell-elements-box a:link, .cell-modul .cell-elements-box a:visited{
	color: #000000; 
}

.cell-elements-box.milestone a{ 
	color: #cc006b;
}


.relationshiptable .even, .overviewtable .even, .listtable .even{
	background-color:#EEFFCC;
}

.relationshiptable .odd, .overviewtable .odd, .listtable odd{
	background-color:#FFFFFF;
}

#navigation {
	float: left;
	background: white;
	width: 210px;
	padding-top: 20px;
	overflow: hidden;
}

body {
	margin: 0px;
}

#navigation ul {
	list-style-type: none;
	padding: 0px;
	margin: 0 0 0 0;
	border-top: 1px solid #008ACD;
}


.h5_menuitem {
	list-style-image: none;
}


#content-small {
	float: left;
	padding: 20px 25px 0px 25px;
	width: 700px;
}


/* EBENE 1 */
.navi-ebene1:link, .navi-ebene1:active, .navi-ebene1:visited {
	font-size: 0.9em;
	color:black;
	text-decoration: none;
	font-weight: bold;
}

.active .navi-ebene1:link, .active .navi-ebene1:active, .active .navi-ebene1:visited{
	color: #008ACD;
}
 
.navi-ebene1-div {
	padding: 0px 0px 2px 0px;
	display: block;
	border-bottom: 1px solid #008ACD;
}

.navi-ebene1-text-div {
	margin: 0px 0px 0px 20px;
	padding: 4px 0px 0px 0px;
}

.navi-ebene1-div.selected {
	background: #99D1EE;
	color: white;
	margin: 0;
}

a.navi-ebene1:active, a.navi-ebene1:hover, a.navi-ebene1:focus, 
.active a.navi-ebene1:active, .active a.navi-ebene1:hover, .active a.navi-ebene1:focus, 
.selected .navi-ebene1:link, .selected .navi-ebene1:active, .selected .navi-ebene1:visited {
	background: #99D1EE;
	color: white;
}

ul li {
	list-style-image: url(Bulletpoint_Hermes.png)
}